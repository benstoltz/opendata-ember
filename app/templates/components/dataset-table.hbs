<h4>Showing  to  of </h4>
<div class="table-responsive">  
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        {{#each model.fields as |field|}}
          <th>
            {{#if field.alias}}
              {{field.alias}}
            {{else}}
              {{field.name}}
            {{/if}}
          </th>
        {{/each}}
      </tr>
    </thead>
    <tbody></tbody>
  </table>
</div>
